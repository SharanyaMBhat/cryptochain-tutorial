":"components/Block.js","original":{"line":43,"column":27}},{"generated":{"line":110,"column":0},"source":"components/Block.js","original":{"line":41,"column":12}},{"generated":{"line":110,"column":22},"source":"components/Block.js","original":{"line":39,"column":18}},{"generated":{"line":110,"column":23},"source":"components/Block.js","original":{"line":39,"column":9}},{"generated":{"line":111,"column":0},"source":"components/Block.js","original":{"line":45,"column":5}},{"name":"Component","generated":{"line":115,"column":2},"source":"components/Block.js","original":{"line":4,"column":20}},{"name":"Block","generated":{"line":117,"column":15},"source":"components/Block.js","original":{"line":70,"column":15}}],"sources":{"components/Block.js":"import React , {Component} from 'react';\nimport {Button} from 'react-bootstrap' \nimport Transaction from './Transaction'\nclass Block extends Component{\n\n    state = {displayTransaction: false};\n\n    toggleTransaction = ()=>{\n        this.setState({displayTransaction: !this.state.displayTransaction})\n    }\n\n\n    get displayTransaction(){\n        const {data} = this.props.block\n\n        const stringifiedData = JSON.stringify(data);\n        const dataDisplay =  stringifiedData.length>35 ? `${stringifiedData.substring(0,35)}...`: stringifiedData;\n\n        if(this.state.displayTransaction){\n            return(\n                <div>\n                    {\n                        data.map(transaction=>(\n                            <div key={transaction.id}>\n                                <hr/>\n                                <Transaction transaction={transaction}/>\n                            </div>\n                        ))\n                    }\n                    <br/>\n                    <Button bsStyle=\"danger\"\n             bsSize=\"small\"\n             onClick={this.toggleTransaction}>Show Less</Button>\n         \n                </div>\n             \n            );\n        }\n         return  (<div>\n            <div>Data: {dataDisplay}</div>\n            <Button bsStyle=\"danger\"\n             bsSize=\"small\"\n             onClick={this.toggleTransaction}>Show More!</Button>\n            </div>) ;\n    }\n    render(){\n        console.log(this.displayTransaction)\n        const {timestamp, hash} = this.props.block\n\n        const hashDisplay = `${hash.substring(0,15)}...`;\n      \n        return<div className='Block'>\n\n            <div>\n                Hash: {hashDisplay}\n            </div>\n\n            <div>\n                Timestamp: {new Date(timestamp).toLocaleString()}\n            </div>\n\n          \n            {this.displayTransaction}\n        </div>\n    }\n\n     \n}\n\nexport default Block"},"lineCount":null}},"hash":"053fc0650d6183d14dcde3d35aa195cc","cacheData":{"env":{}}}